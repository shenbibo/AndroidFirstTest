<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent" >

    <TextView
        android:id="@+id/testDescription"
        android:layout_width="wrap_content"
        android:layout_height="200dp"
        android:layout_alignParentLeft="true"
        android:layout_alignParentRight="true"
        android:layout_alignParentTop="true"
        android:text="TextView" />

    <Button
        android:id="@+id/testButton"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentLeft="true"
        android:layout_below="@+id/testDescription"
        android:layout_marginLeft="37dp"
        android:layout_marginTop="50dp"
        android:text="ObjectTest" />

    <Button
        android:id="@+id/memberTest"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignBaseline="@+id/testButton"
        android:layout_alignBottom="@+id/testButton"
        android:layout_alignParentRight="true"
        android:layout_marginRight="22dp"
        android:text="MemberTest" />

</RelativeLayout>
